   },
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "              North America  Europe   Japan\n",
      "genre                                      \n",
      "Action               879.01  519.13  161.43\n",
      "Sports               684.43  376.79  135.54\n",
      "Shooter              592.24  317.34   38.76\n",
      "Platform             445.50  200.35  130.83\n",
