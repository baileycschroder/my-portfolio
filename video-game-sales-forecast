   "source": [
    "# Analyze patterns in missing values"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {
    "id": "NWeZ6Pj008iW"
   },
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "                 critic_score  user_score  rating\n",
      "year_of_release                                  \n",
      "1980.0                  100.0       100.0     0.0\n",
      "1981.0                  100.0       100.0     0.0\n",
      "1982.0                  100.0       100.0     0.0\n",
      "1983.0                  100.0       100.0     0.0\n",
